alias ll='ls -l'
alias la='ls -ACF'
alias l='ls -CF'
alias dd='dd status=progress'
alias _='sudo'
alias less='less -R'
alias agup='sudo apt-get update'
alias agall='sudo apt-get update; sudo apt-get upgrade -y'
alias ga='git status; git add .; git status'
alias gc='git commit -m'
alias gp='git push'
alias glog='git log --oneline --graph --color --all --decorate'
alias basepiup='cd ~/basepi; git checkout master; git reset --hard origin/master; git pull; ./install.sh; source ~/.bashrc; cd ~'
alias sdd='df -h /'
alias myip="ifconfig | sed -En 's/127.0.0.1//;s/.*inet (addr:)?(([0-9]*\.){3}[0-9]*).*/\2/p'"